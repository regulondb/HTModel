{
    "geneExpression": {
        "validator": {
            "$jsonSchema": {
                "type": "object",
                "required": [
                    "_id",
                    "datasetIds"
                ],
                "properties": {
                    "_id": {
                        "type": [
                            "string",
                            "null"
                        ]
                    },
                    "temporalId": {
                        "type": [
                            "string",
                            "null"
                        ]
                    },
                    "datasetIds": {
                        "type": "array",
                        "uniqueItems": true,
                        "items": {
                            "type": [
                                "string"
                            ]
                        }
                    },
                    "gene": {
                        "type": "object",
                        "properties": {
                            "_id": {
                                "type": [
                                    "string"
                                ]
                            },
                            "name": {
                                "type": [
                                    "string"
                                ]
                            },
                            "synonyms": {
                                "type": "array",
                                "uniqueItems": true,
                                "items": {
                                    "type": [
                                        "string"
                                    ]
                                }
                            },
                            "bnumber": {
                                "type": [
                                    "string",
                                    "null"
                                ]
                            }
                        }
                    },
                    "count": {
                        "type": [
                            "number",
                            "null"
                        ]
                    },
                    "tpm": {
                        "type": [
                            "number",
                            "null"
                        ]
                    },
                    "fpkm": {
                        "type": [
                            "number",
                            "null"
                        ]
                    }
                },
                "additionalProperties": false
            }
        },
        "validationLevel": "strict",
        "validationAction": "error"
    }
}